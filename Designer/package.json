    "amis": "3.4.3",
    "amis-core": "3.4.3",
    "amis-formula": "3.4.3",
    "amis-ui": "3.4.3",
    "amis-editor": "5.6.1",
    "amis-editor-core": "5.6.1",
    "axios": "0.21.2",
    "copy-to-clipboard": "^3.2.0",
